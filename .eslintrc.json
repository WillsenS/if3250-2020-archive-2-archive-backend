{
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier", "jest"],
  "env": {
    "node": true,
    "jest": true
  },
  "rules": {
    "no-await-in-loop": 0,
    "no-console": "off",
    "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "prettier/prettier": ["error"],
    "dot-location": [2, "property"]
  }
}
